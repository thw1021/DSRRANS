FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType RAS;

RAS
{
    RASModel        dsoRStress;

    dsoRStressCoeffs
    {
        alpha1      0.7;
        invScale    on;
    }

    turbulence      on;

    printCoeffs     on;
}

